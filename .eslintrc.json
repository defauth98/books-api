{
    "env": {
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "import/no-unresolved": "off",
        "import/extensions": "off",
        "camelcase":"off",
        "consistent-return":"off",
        "import/prefer-default-export":"off",
        "class-methods-use-this":"off"
    }
}
