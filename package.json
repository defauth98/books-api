  "scripts": {
    "lint": "eslint --fix --ext .js,.jsx .",
    "dev": "ts-node-dev --transpile-only --ignore-watch node_modules src/server.ts",
    "start": "nodemon -x ts-node src/server.ts",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli.js",
    "migrate": "ts-node-dev ./node_modules/typeorm/cli.js -c dev migration:run",
    "revert": "ts-node-dev ./node_modules/typeorm/cli.js -c dev migration:revert",
    "drop": "ts-node-dev ./node_modules/typeorm/cli.js -c dev schema:drop",
    "pretest": "ts-node-dev ./node_modules/typeorm/cli.js -c test migration:run",
    "test": "NODE_ENV=test jest",
    "posttest": "rm ./__tests__/database.sqlite",
    "precoverage": "ts-node-dev ./node_modules/typeorm/cli.js migration:run",
    "coverage": "jest --coverage",
    "postcoverage": "ts-node-dev ./node_modules/typeorm/cli.js schema:drop",
    "test:watch": "NODE_ENV=test jest --watch",
  },
  "license": "MIT",
